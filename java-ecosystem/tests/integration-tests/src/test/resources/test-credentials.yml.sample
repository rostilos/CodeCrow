# CodeCrow Integration Test Credentials
# =====================================
# This file contains credentials for integration tests that require real external services.
# 
# IMPORTANT SECURITY NOTES:
# 1. NEVER commit this file with real credentials to version control
# 2. Copy this file to test-credentials.yml and fill in your values
# 3. The test-credentials.yml file is gitignored
# 4. For CI/CD, use environment variables instead
#
# To use real services in tests, run with profile: -Preal-services

# Bitbucket Cloud Credentials
bitbucket:
  cloud:
    # OAuth credentials for manual connection testing
    oauth:
      client-id: ${BITBUCKET_OAUTH_CLIENT_ID:}
      client-secret: ${BITBUCKET_OAUTH_CLIENT_SECRET:}
      access-token: ${BITBUCKET_ACCESS_TOKEN:}
      refresh-token: ${BITBUCKET_REFRESH_TOKEN:}
    
    # App credentials for app installation testing
    app:
      key: ${BITBUCKET_APP_KEY:}
      secret: ${BITBUCKET_APP_SECRET:}
      client-id: ${BITBUCKET_APP_CLIENT_ID:}
      client-secret: ${BITBUCKET_APP_CLIENT_SECRET:}
    
    # Test repository info
    test-workspace: ${BITBUCKET_TEST_WORKSPACE:}
    test-repo: ${BITBUCKET_TEST_REPO:}

# Bitbucket Server/Data Center Credentials
bitbucket:
  server:
    url: ${BITBUCKET_SERVER_URL:}
    username: ${BITBUCKET_SERVER_USERNAME:}
    password: ${BITBUCKET_SERVER_PASSWORD:}
    personal-token: ${BITBUCKET_SERVER_TOKEN:}

# GitLab Credentials
gitlab:
  # OAuth credentials
  oauth:
    client-id: ${GITLAB_OAUTH_CLIENT_ID:}
    client-secret: ${GITLAB_OAUTH_CLIENT_SECRET:}
    access-token: ${GITLAB_ACCESS_TOKEN:}
  
  # Personal access token
  personal-token: ${GITLAB_PERSONAL_TOKEN:}
  
  # Test repository info
  test-group: ${GITLAB_TEST_GROUP:}
  test-project: ${GITLAB_TEST_PROJECT:}

# GitHub Credentials
github:
  # OAuth App credentials
  oauth:
    client-id: ${GITHUB_OAUTH_CLIENT_ID:}
    client-secret: ${GITHUB_OAUTH_CLIENT_SECRET:}
  
  # GitHub App credentials
  app:
    id: ${GITHUB_APP_ID:}
    private-key: ${GITHUB_APP_PRIVATE_KEY:}
    installation-id: ${GITHUB_APP_INSTALLATION_ID:}
  
  # Personal access token
  personal-token: ${GITHUB_PERSONAL_TOKEN:}
  
  # Test repository info
  test-owner: ${GITHUB_TEST_OWNER:}
  test-repo: ${GITHUB_TEST_REPO:}

# AI Provider Credentials
ai:
  openai:
    api-key: ${OPENAI_API_KEY:}
    model: ${OPENAI_MODEL:gpt-4o-mini}
  
  anthropic:
    api-key: ${ANTHROPIC_API_KEY:}
    model: ${ANTHROPIC_MODEL:claude-3-haiku-20240307}
  
  openrouter:
    api-key: ${OPENROUTER_API_KEY:}
    model: ${OPENROUTER_MODEL:anthropic/claude-3-haiku}

# RAG Pipeline Configuration (for real service testing)
rag:
  pipeline:
    url: ${RAG_PIPELINE_URL:http://localhost:8081}
    api-key: ${RAG_PIPELINE_API_KEY:}

# Inference Orchestrator Configuration (for real service testing)
inference:
  orchestrator:
    url: ${INFERENCE_ORCHESTRATOR_URL:http://localhost:8082}

# Test User Credentials (for auth flow testing)
test-users:
  admin:
    username: ${TEST_ADMIN_USERNAME:admin@test.com}
    password: ${TEST_ADMIN_PASSWORD:TestAdmin123!}
  
  regular:
    username: ${TEST_USER_USERNAME:user@test.com}
    password: ${TEST_USER_PASSWORD:TestUser123!}

# Google OAuth (for social login testing)
google:
  oauth:
    client-id: ${GOOGLE_CLIENT_ID:}
    client-secret: ${GOOGLE_CLIENT_SECRET:}
